{
  "name": "ocat",
  "version": "0.2.1",
  "description": "Inspect an object various ways in order to easily generate test cases.",
  "main": "ocat.js",
  "scripts": {
    "test": "tape test/*.js",
    "lint": "prettier -c test/*.js examples/*.js *.js",
    "lint:fix": "prettier --write test/*.js examples/*.js *.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/ocat.git"
  },
  "homepage": "https://github.com/thlorenz/ocat",
  "dependencies": {
    "xtend": "~4.0.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "tape": "~4.0.0"
  },
  "keywords": [
    "inspect",
    "log",
    "test",
    "generate"
  ],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
